{
  "name": "vue-bootstrap-sidebar",
  "version": "1.0.3",
  "description": "Simple sidebar component for Vue applications build using bootstrap-vue",
  "author": "Juraj Kavka <jurajkavka@gmail.com",
  "license": "MIT",
  "main": "dist/vue-bootstrap-sidebar.ssr.js",
  "module": "dist/vue-bootstrap-sidebar.esm.js",
  "unpkg": "dist/vue-bootstrap-sidebar.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "src/**/*.scss",
    "!src/serve-dev.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JurajKavka/vue-bootstrap-sidebar.git"
  },
  "keywords": [
    "vue",
    "vue-bootstrap",
    "vue-bootstrap-sidebar",
    "vue-bootstrap-sidebar-menu",
    "vue-component",
    "vue-sidebar",
    "vue-sidebar-menu",
    "sidebar",
    "sidebar-menu",
    "admin-sidebar",
    "bootstrap",
    "bootstrap-vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "echo \"No test specified\"",
    "lint:script": "eslint --ext .vue,.js src/",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "lint": "npm run lint:script; npm run lint:style"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "minimist": "^1.2.0",
    "node-sass": "^4.13.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "5.1.1",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0"
  },
  "engines": {
    "node": ">=8"
  }
}
